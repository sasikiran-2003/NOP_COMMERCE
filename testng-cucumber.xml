<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="nopCommerce Cucumber Tests">
    <parameter name="browser" value="chrome"/>
    
    <test name="Cucumber Tests">
        <classes>
            <class name="com.nopcommerce.tests.cucumber.runner.TestRunner"/>
        </classes>
    </test>
    
    <test name="Failed Cucumber Tests">
        <classes>
            <class name="com.nopcommerce.tests.cucumber.runner.FailedTestRunner"/>
        </classes>
    </test>
</suite>